@model List<model.entity.Propietario>
@{
    ViewBag.Title = "Inicio";
}

<h2>Propietario</h2>
<div class="container-fluid">
    <div>
        <a class="btn btn-primary" href="/Propietario/Create">Nuevo</a>
    </div>
    <table class="table">
        <tr class="btn-primary">
            <th>Nombre</th>
            <th>Tipo de documento</th>
            <th>Valor documento</th>
            <th colspan="3"> Operaciones</th>
        </tr>
        @foreach (var objetoPropietario in Model)
        {
            <tr>
                <td>@objetoPropietario.Nombre</td>
                <td>@objetoPropietario.TipoDocumento</td>
                <td>@objetoPropietario.ValorDocumentoId</td>
                <td><a class="btn btn-success" href="~/Propietario/Find/@objetoPropietario.IdPropietario">Ver</a></td>
                <td><a class="btn btn-warning" href="~/Propietario/Update/@objetoPropietario.IdPropietario">Editar</a></td>
                <td><a class="btn btn-danger" href="~/Propietario/Delete/@objetoPropietario.IdPropietario">Borrar</a></td>
            </tr>
        }
    </table>
</div>

