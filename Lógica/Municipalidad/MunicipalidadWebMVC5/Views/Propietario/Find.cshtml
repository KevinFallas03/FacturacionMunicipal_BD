@model model.entity.Propietario
@{
    ViewBag.Title = "Find";
}

<h2>Datos de Propietario</h2>
@using (Html.BeginForm())
{
<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(model => model.IdPropietario, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.IdPropietario, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Nombre, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.TipoDocumento, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.TipoDocumento, new { htmlAttributes = new { @class = "form-control", @value="return " } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ValorDocumentoId, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.ValorDocumentoId, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
</div>
<div class="form-actions col-md-offset-2">
    <a class="btn btn-primary" href="/Propietario/Responsable/@Model.IdPropietario">Responsable</a>
    <input class="btn btn-primary" type="submit" name=btnprop id="prp" value="Propiedades" href="Propietario/Propiedades" />
    @Html.ActionLink("Regresar al listado", "Inicio")
</div>
}



