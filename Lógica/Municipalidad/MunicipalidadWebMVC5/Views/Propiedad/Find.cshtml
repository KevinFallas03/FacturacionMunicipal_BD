@model model.entity.Propiedad
@{
    ViewBag.Title = "Find";
}

<h2>Datos de Propiedad</h2>
@using (Html.BeginForm())
{
<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(model => model.IdPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.IdPropiedad, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.NumeroPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.NumeroPropiedad, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ValorPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.ValorPropiedad, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.DireccionPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-6">
            @Html.DisplayFor(model => model.DireccionPropiedad, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>

    <div class="form-actions col-md-offset-2">
        <a class="btn btn-primary" href='~/Propiedad/Usuarios/@Model.IdPropiedad'>Usuarios</a>
        <a class="btn btn-primary" href='~/Propiedad/Propietarios/@Model.IdPropiedad'>Propietarios</a>
        @Html.ActionLink("Regresar al listado", "Inicio")
    </div>



</div>

}
