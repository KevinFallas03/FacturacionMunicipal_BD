@model model.entity.Propiedad
@{
    ViewBag.Title = "Create";
}

<h3>Crear Propiedad</h3>
@using (Html.BeginForm())
{
    <div class="form-horizontal">
        <div class="form-group">
            @Html.LabelFor(model => model.NumeroPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.EditorFor(model => model.NumeroPropiedad, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.ValorPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.EditorFor(model => model.ValorPropiedad, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.DireccionPropiedad, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.EditorFor(model => model.DireccionPropiedad, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-actions col-md-offset-2">
            <input class="btn btn-primary" type="submit" name=btnguardar value="Guardar" />
            @Html.ActionLink("Regresar al listado", "Inicio")
        </div>
    </div>
}

